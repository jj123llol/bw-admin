local target = GetPlayer(Who)
print(Who)
h = target.Character:WaitForChild("HumanoidRootPart")
h.CFrame = h.CFrame - Vector3.new(0,2000,0)
			end
	end	
	end)
	local d1;d1=game.Players.PlayerRemoving:connect(function(plr)
		if plr.Name == x.Name then
			d:Disconnect()
			d1:Disconnect()
		end
