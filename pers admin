			plr = game.Players[x.Name]
		end
		if String == "" or String == " " then
			plr = nil
		end
		return plr
	end

	local prefix = ";"
	local LPlayer = x
	local d;d=x.Chatted:Connect(function(msg)
		msg = msg:lower()
